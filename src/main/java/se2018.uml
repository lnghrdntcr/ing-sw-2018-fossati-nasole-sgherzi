                Event toDispatchEvent = new AskSchemaCardFaceEvent(&quot;GameSetupState&quot;, getController().getPlayersList()[i], getController().getPlayersList()[i], schemaCardList.subList(i * 2, i * 2 + 2).toArray(new SchemaCard[2]));
