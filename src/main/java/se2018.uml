    localProxy.setView(virtualView);
    virtualViews.add(virtualView);
    if (virtualViews.size() == 4 &amp;&amp; !gameStarted) {
        startGame();
        return;
    }
    if (virtualViews.size() == 2)
        this.startTimeout();
</body>
